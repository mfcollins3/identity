{
    "name": "identity",
    "image": "mcr.microsoft.com/devcontainers/base:1.0.9-bookworm",
    "features": {
        "ghcr.io/devcontainers/features/node:1": {
            "version": "20.11.1"
        },
        "ghcr.io/devcontainers/features/rust:1": {
            "version": "1.77.1"
        }
    },
    "postCreateCommand": ". ${NVM_DIR}/nvm.sh && ./install.sh",
    "customizations": {
        "vscode": {
            "extensions": [
                "rust-lang.rust-analyzer",
                "vadimcn.vscode-lldb"
            ]
        }
    }
}